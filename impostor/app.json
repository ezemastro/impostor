{
  "expo": {
    "name": "impostor",
    "slug": "impostor",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/impostor_icon_v3.png",
    "scheme": "impostor",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/impostor_icon_v3.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/impostor_icon_v3.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.ezemastro.impostor"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/impostor_icon_v3.png"
    },
    "plugins": [
      "expo-router",
      "expo-mail-composer"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "aec0b482-547a-47b7-9353-01b12e0492d8"
      }
    },
    "owner": "ezemastro"
  }
}
